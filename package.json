{
	"name": "oritsy",
	"version": "1.0.0",
	"description": "One stop shop for homemade, vintage, and unique hand crafted products for the cheapest prices.",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node back-end/server",
		"server": "nodemon back-end/server",
		"client": "npm start --prefix front-end",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:insert": "node back-end/dataFeeder",
		"data:delete": "node back-end/dataFeeder -d"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SubashRandika/oritsy.git"
	},
	"keywords": [],
	"author": "Randika Rodrigo",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/SubashRandika/oritsy/issues"
	},
	"homepage": "https://github.com/SubashRandika/oritsy#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongoose": "^5.12.3"
	},
	"devDependencies": {
		"concurrently": "^6.0.1",
		"nodemon": "^2.0.7"
	}
}
